    <th></th>
  </tr>

<% @data.each do |datum| %>
  <tr>
    <td><%= datum.param1 %></td>
    <td><%= datum.param2 %></td>
    <td><%= datum.param3 %></td>
    <td><%= datum.param4 %></td>
    <td><%= datum.param5 %></td>
    <td><%= datum.param6 %></td>
    <td><%= datum.param7 %></td>
    <td><%= datum.param8 %></td>
    <td><%= datum.param9 %></td>
    <td><%= datum.param10 %></td>
    <td><%= link_to 'Show', datum %></td>
    <td><%= link_to 'Edit', edit_datum_path(datum) %></td>
    <td><%= link_to 'Destroy', datum, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

