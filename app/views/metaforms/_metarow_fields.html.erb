<fieldset>
  <%= f.label :key, "Key" %>
  <%= f.text_field :key %>
  
  <%= f.label :value, "Value" %>
  <%= f.text_field :value, :size => nil %>
  
  <%= f.label :autofill, "Autofill" %>
  <%= f.check_box :autofill %><br /><br />
  
  <%= f.hidden_field :_destroy %>
  <%= link_to_function "Remove Metadata", "del_mrow(this)" %>  
  <%#= link_to "Remove", '#', class: "remove_fields" %>
  <hr>
</fieldset>
