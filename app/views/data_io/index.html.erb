
<h3>Document Upload and Import</h3>
 <%= form_for :dump, :url=>{:controller=>"DataIO", :action=>"csv_import"}, :html => { :multipart => true } do |f| -%>
 <table>
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
   </tr>
   <tr>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr>
     <td>
     	<%= select("post", "ifilter_id", Ifilter.all.collect {|f| [ f.name, f.id ] }, {:prompt => 'Select Filter'})%>
     </td>
   </tr>
   <tr>
     <td>
         <%= f.check_box :contains_header, :checked => true %> File has header
     </td>
   </tr>
   <tr>
   	 <td>
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end -%>

<h3>Existing Documents</h3>
<p>Download, export, and visualize data from the <%= link_to 'Documents', documents_path %> page </p>
