<div id="notice"><%= flash.now[:message] %></div>

<h3>Input</h3>
 <%= form_for :dump, :url=>{:controller=>"DataIO", :action=>"csv_import"}, :html => { :multipart => true } do |f| -%>
 <table">
   <tr>
     <td>
      <label for="dump_file">
        Select a CSV File :
      </label>
     </td>
     <td >
       <%= f.file_field :file -%>
     </td>
   </tr>
   <tr>
     <td colspan='2'>
       <%= submit_tag 'Submit' -%>
     </td>
   </tr>
 </table>
<% end -%>

<h3>Saved</h3>
<ul>
	<li><%= link_to "Uploads", "/Metadata#index" %></li>		
</ul>

<h3>Output</h3>
<ul>
	<li><%= link_to "Export", "#"%></li>
</ul>